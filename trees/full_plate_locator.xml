<root BTCPP_format="4" main_tree_to_execute="FullPlateLocator" >
  <!-- Subtree Includes -->
  <include path="./helpers.xml" />

  <!-- Simple Trajectories for Moving Once for Locating Full Plate from Original Plate Position; used in both Full and Shared Autonomy -->
  <BehaviorTree ID="FullPlateLocator">
    <Sequence name="root_sequence">
      <!-- Go to forward right -->
      <AdaPlanToPoseOffset offset="0.05;0.35;0.0" rotation="0.0;0.0;0.5" worldCollision="true" traj="{traj}" />
      <AdaExecuteTrajectory traj="{traj}" />
    </Sequence>
  </BehaviorTree>
</root>